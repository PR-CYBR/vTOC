# Required database configuration
postgres_database       = "vtoc"
postgres_user           = "vtoc"
postgres_password       = "changeme-database-password"
postgres_host           = "postgres.example.internal"
backend_public_base_url = "https://api.example.com"

# Frontend widgets and integrations
chatkit_api_key            = "changeme-chatkit-public-key"
chatkit_org_id             = "changeme-chatkit-org"
chatkit_webhook_secret     = "changeme-chatkit-webhook"
agentkit_api_key           = "changeme-agentkit-key"
agentkit_org_id            = "changeme-agentkit-org"
supabase_project_ref       = "your-supabase-project-ref"
supabase_api_url           = "https://your-project.supabase.co"
supabase_anon_key          = "changeme-supabase-anon"
supabase_service_role_key  = "changeme-supabase-service-role"
supabase_jwt_secret        = "changeme-supabase-jwt"
fly_app_name               = "vtoc-app"
fly_api_token              = "changeme-fly-token"
ghcr_deploy_username       = "github-user"
ghcr_deploy_token          = "changeme-ghcr-token"

# Optional overrides
postgres_internal_host     = "database"
frontend_port              = 5173
frontend_default_div       = "PR-SJU"
backend_image_repository   = "ghcr.io/pr-cybr/vtoc/backend"
frontend_image_repository  = "ghcr.io/pr-cybr/vtoc/frontend"
scraper_image_repository   = "ghcr.io/pr-cybr/vtoc/scraper"
zerotier_network_id        = ""
tailscale_auth_key         = ""
additional_fly_secrets     = {}
additional_frontend_env    = {}

# Telemetry ingest defaults
gps_serial_device          = "/dev/ttyUSB0"
gps_baud_rate              = 9600
gps_source_slug            = "station-gps"
adsb_feed_url              = "http://dump1090:8080/data/aircraft.json"
adsb_source_slug           = "station-adsb"
rtlsdr_device_serial       = ""
h4m_serial_device          = "/dev/ttyUSB1"
h4m_baud_rate              = 115200
h4m_channel                = "station-h4m"
h4m_source_slug            = "station-h4m"
